<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="urn:Testing_Service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/" xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/" xmlns="http://schemas.xmlsoap.org/wsdl/" targetNamespace="urn:Testing_Service" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
  <wsdl:types>
    <xsd:schema targetNamespace="urn:Testing_Service">
      <xsd:import namespace="http://schemas.xmlsoap.org/soap/encoding/" />
      <xsd:import namespace="http://schemas.xmlsoap.org/wsdl/" />
    </xsd:schema>
  </wsdl:types>
  <wsdl:message name="Say_HelloRequest">
    <wsdl:part name="name" type="xsd:string" />
  </wsdl:message>
  <wsdl:message name="Say_HelloResponse">
    <wsdl:part name="return" type="xsd:string" />
  </wsdl:message>
  <wsdl:portType name="Testing_ServicePortType">
    <wsdl:operation name="Say_Hello">
      <documentation>Sends a greeting with your name!</documentation>
      <wsdl:input message="tns:Say_HelloRequest" />
      <wsdl:output message="tns:Say_HelloResponse" />
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="Testing_ServiceBinding" type="tns:Testing_ServicePortType">
    <soap:binding transport="http://schemas.xmlsoap.org/soap/http" style="rpc" />
    <wsdl:operation name="Say_Hello">
      <soap:operation soapAction="urn:Testing_Service#hello" style="rpc" />
      <wsdl:input>
        <soap:body use="encoded" namespace="urn:Testing_Service" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
      </wsdl:input>
      <wsdl:output>
        <soap:body use="encoded" namespace="urn:Testing_Service" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="Testing_Service">
    <wsdl:port name="Testing_ServicePort" binding="tns:Testing_ServiceBinding">
      <soap:address location="http://localhost/studio/retail/retailVI/My_Test_Service.php" />
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>